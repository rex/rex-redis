{
  "author": "Pierce Moore <me@prex.io>",
  "name": "rex-redis",
  "description": "A small collection of utilities to simplify development in a Redis-powered environment.",
  "homepage": "https://github.com/rex/rex-redis",
  "version": "0.2.0",
  "main": "lib/rex-redis.js",
  "bin": {
    "rex-redis": "bin/rex-kickstart",
    "rredis": "bin/rex-kickstart"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rex/rex-redis.git"
  },
  "keywords": [
    "redis",
    "utility",
    "cli",
    "shell",
    "rex"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "rex-exec": "*",
    "rex-shell": "*",
    "rex-utils": "*",
    "optimist": "*"
  },
  "devDependencies": {
    "async": "*"
  },
  "config": {
    "cli" : {
      "usage" : "rex-redis [clear|count|help]",
      "args" : {
        "clear   " : "Clear all active keys/objects in Redis cache.",
        "count   " : "Count all active keys/objects in Redis cache."
      }
    }
  },
  "bugs": {
    "url": "https://github.com/rex/rex-redis/issues"
  },
  "scripts": {
    "test": "node ./test/rex-redis.test.js"
  }
}
